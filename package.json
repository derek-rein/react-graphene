{
  "name": "react-graphene",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "start-storybook -p 6006",
    "lint": "rome check ./src/**/* --help",
    "build": "yarn lint && yarn build:vite && yarn build:storybook",
    "deploy:sb": "gh-pages -d storybook-static",
    "build:vite": "vite build",
    "build:sb": "build-storybook --quiet --loglevel error --output-dir ./storybook-static",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf ./storybook-static"
  },
  "license": "MIT",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.ts|tsx": "eslint --fix"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "styled-components": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@commitlint/config-conventional": "^17.1.0",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.10",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitejs/plugin-react": "^2.0.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "babel-loader": "^8.2.5",
    "camelcase": "^7.0.0",
    "date-fns": "^2.29.2",
    "lint-staged": "^13.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.54.8",
    "styled-components": "^5.3.5",
    "typescript": "^4.8.2",
    "use-resize-observer": "^9.0.2",
    "vite": "^3.0.9",
    "vite-plugin-dts": "^1.4.1",
    "vite-plugin-eslint": "^1.8.1",
    "gh-pages": "^4.0.0",
    "rome": "^10.0.1"
  }
}
