{
  "name": "react-graphene",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "storybook dev -p 6006",
    "lint": "pnpm biome check --write ./src",
    "build": "pnpm run lint && pnpm run build:vite && pnpm run build:sb",
    "deploy:sb": "gh-pages -d storybook-static",
    "build:vite": "vite build",
    "build:sb": "storybook build --quiet --loglevel error --output-dir ./storybook-static",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf ./storybook-static"
  },
  "license": "MIT",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "styled-components": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@biomejs/biome": "1.9.4",
    "@chromatic-com/storybook": "^3",
    "@commitlint/config-conventional": "^17.8.1",
    "@storybook/addon-actions": "^8.6.12",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-links": "^8.6.12",
    "@storybook/addon-mdx-gfm": "^8.6.12",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^8.6.12",
    "@storybook/react-vite": "^8.6.12",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "babel-loader": "^8.4.1",
    "date-fns": "^2.30.0",
    "gh-pages": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sass": "^1.87.0",
    "storybook": "^8.6.12",
    "styled-components": "^6.1.17",
    "typescript": "^5.8.3",
    "use-resize-observer": "^9.1.0",
    "vite": "^6.3.4",
    "vite-plugin-dts": "^1.7.3"
  },
  "dependencies": {
    "camelcase": "^8.0.0"
  }
}
